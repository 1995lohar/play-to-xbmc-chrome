{
    "manifest_version": 2,
    "version": "1.2.5.1",
    "name": "Play to XBMC",
    "description": "Send YouTube, Vimeo or CollegeHumor videos to your XBMC Player.",
    "options_page": "options.html",
    "permissions": [ "tabs", "http://*/*" ],
    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "remote.html"
    },
    "icons": {
        "128": "images/tv.png"
    }
}
